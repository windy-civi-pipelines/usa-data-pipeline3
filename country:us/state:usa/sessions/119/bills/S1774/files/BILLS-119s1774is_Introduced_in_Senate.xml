        <action>
            <action-date date="20250515">May 15, 2025</action-date>
            <action-desc><sponsor name-id="S323">Mr. Risch</sponsor> (for himself, <cosponsor name-id="S410">Ms. Lummis</cosponsor>, <cosponsor name-id="S423">Mr.
                    Ricketts</cosponsor>, and <cosponsor name-id="S398">Mr. Cramer</cosponsor>)
