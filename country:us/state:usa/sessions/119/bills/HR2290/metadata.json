  "identifier": "HR 2290",
  "title": "World War II Women's Memorial Location Act",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "To provide that the memorial to commemorate the sacrifice and service of the women who worked on the home front to support the efforts of the United States military during World War II may be located on the National Mall, and for other purposes."
    }
  ],
